{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "mltd-oscillo",
  "version": "0.1.2",
  "identifier": "com.mltd-oscillo.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "mltd-oscillo",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "capabilities": ["default"],
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
